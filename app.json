{
  "expo": {
    "name": "Notes",
    "description": "Test release.",
    "slug": "notes",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["android"],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/sticky_note.png",
    "scheme": "notes",
    "androidStatusBar": {
      "backgroundColor": "#1a72b4"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 8,

      "permissions": ["VIBRATE", "NOTIFICATIONS", "ACCESS_NETWORK_STATE"],
      "package": "com.mturay.notes"
    }
  }
}
